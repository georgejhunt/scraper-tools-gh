.audioplayercomponent .audioplayer {
    max-width: 480px;
    height: 60px;
    margin: 50px auto auto;
    box-shadow: 0 18px 23px #00000059;
    border-radius: 7px;
    padding: 8px;
}

.audioplayercomponent .pButton {
    height:50px; 
    width: 50px;
    border: none;
    background-size: 50% 50%;
    background-repeat: no-repeat;
    background-position: center;
    background-color: transparent;
    float:left;
    outline:none;
    margin-top: 5px;
}

.audioplayercomponent .play{background-image: url('https://media.gcflearnfree.org/assets/es-audio-controls/play.png');}
.audioplayercomponent .pause{background-image: url('https://media.gcflearnfree.org/assets/es-audio-controls/pause.png');}

.audioplayercomponent .timeline {
    max-width: 400px;
    height: 10px;
    margin-top: 25px;
    float: left;
    border-radius: 15px;
    width: 79%;
    background: #0000000d;
}

.audioplayercomponent .playhead{
    width: 18px;
    height: 18px;
    border-radius: 50%;
    margin-top: -4px;
    background: #38cfca;
}

@media only screen and (max-width:280px){
    .audioplayercomponent .timeline {
        width: 50%;
    }
}
